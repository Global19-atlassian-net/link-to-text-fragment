{
  "name": "link-to-text-fragment",
  "version": "1.1.4",
  "description": "Browser extension that allows for linking to arbitrary text fragments.",
  "scripts": {
    "lint": "npx eslint ./*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomayac/link-to-text-fragment.git"
  },
  "keywords": [
    "text fragment",
    "link to text fragment",
    "scroll to text fragment"
  ],
  "author": "Thomas Steiner (https://blog.tomayac.com/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tomayac/link-to-text-fragment/issues"
  },
  "homepage": "https://github.com/tomayac/link-to-text-fragment#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "google"
    ]
  }
